{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "prettier",
    "import"
  ],
  "rules": {
    "max-depth": [
      "error",
      2
    ],
    "max-lines-per-function": [
      "error",
      10
    ]
  }
}
